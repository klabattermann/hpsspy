        print(f)
    for root, dirs, files in walk('test/'):
        print(root)
        print(dirs)
        print(files)
    #out,err = htar('-t', '-f', 'boss/dr8/boss/calib.tar')
    #print('STDOUT=')
    #print(out)
    #print('STDERR=')
    #print(err)
    return 0
