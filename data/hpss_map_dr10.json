{
  "exclude":[
    "env/index.html"
  ],
  "regex":{
    "casload":{
      "casload/(apogee|galaxy|sp)CSV/.*$":"casload/\\1CSV.tar",
      "casload/wiseCSV/parts_([0-9]+)_([0-9]+)/.*$":"casload/wiseCSV/parts_\\1_\\2.tar",
      "casload/wiseCSV/xmatch/.*$":"casload/wiseCSV/xmatch.tar"
    },
    "apogee":{
      "apogee/target/.*$":"apogee/apogee_target.tar",
      "apogee/spectro/data/([0-9]{5})/.*$":"apogee/spectro/data/apogee_spectro_data_\\1.tar",
      "apogee/spectro/redux/r3/[^/]+$":"apogee/spectro/redux/r3/r3_files.tar",
      "apogee/spectro/redux/r3/cal/[^/]+$":"apogee/spectro/redux/r3/cal/r3_cal_files.tar",
      "apogee/spectro/redux/r3/cal/([a-z]+)/.*$":"apogee/spectro/redux/r3/cal/r3_cal_\\1.tar",
      "apogee/spectro/redux/r3/(fields|plates|red)/([0-9]+)/.*$":"apogee/spectro/redux/r3/\\1/r3_\\1_\\2.tar",
      "apogee/spectro/redux/r3/s3/([0-9]+|plan)/.*$":"apogee/spectro/redux/r3/s3/r3_s3_\\1.tar",
      "apogee/spectro/redux/r3/s3/a3/([0-9]+C?|plan)/.*$":"apogee/spectro/redux/r3/s3/a3/r3_s3_a3_\\1.tar",
      "apogee/spectro/redux/r3/s3/a3/v304/[^/]+$":"apogee/spectro/redux/r3/s3/a3/v304/r3_s3_a3_v304_files.tar",
      "apogee/spectro/redux/r3/s3/a3/v304/([0-9]+|html|log|plan)/.*$":"apogee/spectro/redux/r3/s3/a3/v304/r3_s3_a3_v304_\\1.tar",
      "apogee/spectro/redux/speclib/.*$":"apogee/spectro/redux/speclib.tar"
    },
    "boss":{
      "boss/lss/.*$":"boss/lss.tar",
      "boss/papers/.*$":"boss/papers.tar",
      "boss/photoObj/photoRunAll[^/]+$":"boss/photoObj/boss_photoObj_files.tar",
      "boss/photoObj/[^/]+\\.sha1sum$":"boss/photoObj/boss_photoObj_files.tar",
      "boss/photoObj/external/WISE/301/([0-9]+)/.*$":"boss/photoObj/external/WISE/301/WISE_301_\\1.tar",
      "boss/photoObj/plates/.*$":"boss/photoObj/boss_plates.tar",
      "boss/qso/.*$":"boss/qso.tar",
      "boss/spectro/data/([0-9]{5})/.*$":"boss/spectro/data/boss_spectro_data_\\1.tar",
      "boss/spectro/redux/galaxy/v1_0/[^/]+$":"boss/spectro/redux/galaxy/v1_0/galaxy_v1_0_v5_5_12_files.tar",
      "boss/spectro/redux/galaxy/v1_0/v5_5_12/([0-9]+)/.*$":"boss/spectro/redux/galaxy/v1_0/v5_5_12/galaxy_v1_0_v5_5_12_\\1.tar",
      "boss/spectro/redux/images/v5_5_12/v5_5_12/([0-9]+)-([0-9]+)/.*$":"boss/spectro/redux/images/v5_5_12/v5_5_12/redux_images_v5_5_12_v5_5_12_\\1-\\2.tar",
      "boss/spectro/redux/v5_5_12/[^/]+$":"boss/spectro/redux/v5_5_12/redux_v5_5_12_files.tar",
      "boss/spectro/redux/v5_5_12/([0-9]+)/.*$":"boss/spectro/redux/v5_5_12/redux_v5_5_12_\\1.tar",
      "boss/spectro/redux/v5_5_12/spectra/([0-9]+)/.*$":"boss/spectro/redux/v5_5_12/spectra/spectra_v5_5_12_\\1.tar",
      "boss/spectro/redux/v5_5_12/spectra/lite/([0-9]+)/.*$":"boss/spectro/redux/v5_5_12/spectra/lite/spectra_lite_v5_5_12_\\1.tar",
      "boss/sweeps/dr9/external/WISE/.*$":"boss/sweeps/dr9/external/WISE.tar"
    },
    "sdss":{
      "sdss/spectro/redux/(plates|specObj)(-SDSS|-BOSS|)-dr[0-9]+\\.(fits|par)$":"sdss/spectro/redux/redux_files.tar",
      "sdss/spectro/redux/photo(Match|Pos|)Plate-dr[0-9]+\\.fits$":"sdss/spectro/redux/redux_files.tar",
      "sdss/spectro/redux/[^/]+\\.sha1sum$":"sdss/spectro/redux/redux_files.tar",
      "sdss/spectro/redux/galaxy/[^/]+$":"sdss/spectro/redux/galaxy/redux_galaxy_files.tar",
      "sdss/spectro/redux/galaxy/v1_0/[^/]+$":"sdss/spectro/redux/galaxy/v1_0/redux_galaxy_v1_0_files.tar",
      "sdss/spectro/redux/galaxy/v1_0/([0-9]+)/([0-9]+)/.*$":"sdss/spectro/redux/galaxy/v1_0/\\1/galaxy_v1_0_\\1_\\2.tar"
    }
  }
}
